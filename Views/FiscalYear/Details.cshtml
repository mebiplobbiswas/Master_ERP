@model MASERP.Models.DATAMODEL.FiscalYear

@{
    ViewBag.Title = "Fiscal Year Details";
}

<h2>Fiscal Year Details</h2>

<table class="table table-bordered">
    <tr>
        <th>Year Name</th>
        <td>@Model.YearName</td>
    </tr>
    <tr>
        <th>Start Date</th>
        <td>@Model.StartDate.ToShortDateString()</td>
    </tr>
    <tr>
        <th>End Date</th>
        <td>@Model.EndDate.ToShortDateString()</td>
    </tr>
    <tr>
        <th>Is Active</th>
        <td>@(Model.IsActive ? "Yes" : "No")</td>
    </tr>
</table>